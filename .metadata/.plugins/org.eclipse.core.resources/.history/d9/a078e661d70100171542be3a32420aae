package JavaProgram;

public class FindOutMaxTwoNO {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int[] a={1,2,3,4,5,6};
		int m1=0;
		int m2=0;
		
		int maxOne = 0;
		 int maxTwo = 0;
		 
		for(int i:a){
		 
		    if(maxOne < i){
		           maxTwo = maxOne;
		           maxOne =i;
		     } else if(maxTwo < i){
		                maxTwo = i;
		     }
		}
		System.out.println(maxOne);
		System.out.println(maxTwo);
	}

}
