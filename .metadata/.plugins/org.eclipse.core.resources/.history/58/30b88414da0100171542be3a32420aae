package JavaProgram;

public class EvenOrOaddwothoutusing {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int n=10;
		
		while(n>=0)
		{
			if(n==0)
			{
				System.out.println("even");
			}
			if(n==1)
			{
				System.out.println("odd");
			}
			n=n-2;
		}
	}

}
